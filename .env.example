# Cloudflare R2 Configuration
R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your-r2-access-key-id
R2_SECRET_ACCESS_KEY=your-r2-secret-access-key
R2_BUCKET_NAME=your-bucket-name

# OneDrive Authentication (Optional but recommended)
# If not provided, script will attempt anonymous access which may fail
ONEDRIVE_EMAIL=your-email@example.com
ONEDRIVE_PASSWORD=your-password

# Puppeteer Configuration (Optional)
PUPPETEER_HEADLESS=true
# PUPPETEER_EXECUTABLE_PATH=/path/to/chrome 